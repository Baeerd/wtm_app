                                            "                        } else {\r",
                                            "                            Ext.Msg.alert('错误','连接异常！',Ext.emptyFn);\r",
                                            "                        }\r",
                                            "                    }\r",
                                            "                });\r",
                                            "            } else {\r",
                                            "                if (Ext.os.is('Android')) {\r",
                                            "                    window.plugins.toast.show('钢卷号扫描失败！', 'long', 'center',\r",
                                            "                                              function(a){console.log('toast success: ' + a);},\r",
                                            "                                              function(b){console.log('toast error: ' + b);});\r",
