                                            "                        var obj = Ext.decode(text);\r",
                                            "                        console.log('conn = ',conn.responseText);\r",
                                            "                        console.log('obj = ',obj);\r",
                                            "                        if (obj.results===1) {\r",
                                            "                            currentView.down('#weight').setValue(obj.items[0].matWeight+'t');\r",
                                            "                            currentView.down('#standard').setValue(obj.items[0].matWidth+\r",
                                            "                                                                  'mm x'+obj.items[0].matThickness+'mm');\r",
                                            "                            currentView.down('#type').setValue(obj.items[0].steelGrade);\r",
                                            "                            currentView.down('#current').setValue(obj.items[0].slotId);\r",
                                            "\r",
                                            "                        } else {\r",
                                            "                            if (Ext.os.is('Android')) {\r",
                                            "                                window.plugins.toast.show('没有匹配的结果！', 'long', 'center', \r",
